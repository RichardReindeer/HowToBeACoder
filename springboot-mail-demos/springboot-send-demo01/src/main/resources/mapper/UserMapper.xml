<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bambi.springboot.send.demo01.mapper.UserMapper">
    <select id="selectUserByUsername" resultType="com.bambi.springboot.send.demo01.vo.UserVo" parameterType="java.lang.String">
        select * from user where username = #{username}
    </select>
</mapper>